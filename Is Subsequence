// Approach 1
if(s.size()==0 && t.size()==0)
        {
            return true;
        }
        if(t.size()==0)
        {
            return false;
        }
	        int i=0;
	        int cnt=0;
	        for(int j=0; j<t.size(); j++)
	        {
	            if(t[j]==s[i])
	            {
	                i++;
	                cnt++;
	            }
	        }
	        if(cnt==s.size())
	        {
	            return true;
	        }
	        return false;
