// Approach 1 -> TC-> O(N^2)
-> Generate all the subarrays and whose sum is maximum print that.

// Approach 2 -> TC-> O(N) SC->O(1)

int sum=0;
	    int max_sum=INT_MIN:
	    for(int i=0; i<arr.size(); i++)
	    {
	        sum+=arr[i];
	        if(max_sum<sum)
	        {
	            max_sum=sum;
	        }
	        if(sum<0)
	        {
	            sum=0;
	        }
	    }
	    return max_sum;
